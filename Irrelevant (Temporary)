num = []

while True:
    x = int(input())
    num.append(x)
    if len(num) >= 5:
        while True:
            z = input("Do you want to stop? ")
            if z != "Yes" and z != "No" and z != "yes" and z != "no":
                print("Please answer Yes or No.")
            else:
                break
        if z == "Yes" or z == "yes":
            break

print("The sum is "+str(sum(num)))
